{
    "fullName": "Something DBC",
    "flow": {
        "deploy": ["nightly"]
    },
    "pipelines": [
        {
            "type": "APP",
            "tool": {
                "type": "maven",
                "version": "3.0.1"
            },
            "technology": {
                "type": "jdk",
                "version": "6u45"
            },
            "scm": {
                "type": "svn",
                "base": "https://svn.company.tld/group/dbc"
            },
            "ci": {
                "maven": {
                    "rootpom": "pom.xml",
                    "goals": "clean findbugs:findbugs cobertura:cobertura dependency:list package",
                    "opts": "-DoutputFile=dependencies.txt"
                },
                "cobertura": {
                    "coberturaReportFile": "**/target/site/cobertura/coverage.xml",
                    "onlyStable": "false",
                    "failUnhealthy": "false",
                    "failUnstable": "false",
                    "autoUpdateHealth": "false",
                    "autoUpdateStability": "false",
                    "zoomCoverageChart": "false",
                    "failNoReports": "false",
                    "sourceEncoding": "ASCII",
                    "targets": [
                        {
                            "type": "healthy",
                            "method": "80",
                            "line": "80",
                            "conditional": "70"
                        },
                        {
                            "type": "unhealthy",
                            "method": "0",
                            "line": "0",
                            "conditional": "0"
                        },
                        {
                            "type": "failing",
                            "method": "0",
                            "line": "0",
                            "conditional": "0"
                        }
                    ]
                },
                "dry": {
                    "healthy": "",
                    "unhealthy": "",
                    "thresholdLimit": "low",
                    "defaultEncoding": "",
                    "canRunOnFailed": "",
                    "useStableBuildAsReference": "false",
                    "useDeltaValues": "",
                    "thresholds": {
                        "unstableTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "unstableNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        }
                    },
                    "shouldDetectModules": "",
                    "dontComputeNew": "",
                    "doNotResolveRelativePaths": "",
                    "highThreshold": "50",
                    "normalThreshold": "25"
                },
                "pmd": {
                    "healthy": "",
                    "unhealthy": "",
                    "thresholdLimit": "low",
                    "defaultEncoding": "",
                    "canRunOnFailed": "",
                    "useStableBuildAsReference": "false",
                    "useDeltaValues": "",
                    "thresholds": {
                        "unstableTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedTotal": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "unstableNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        },
                        "failedNew": {
                            "all": "",
                            "high": "",
                            "normal": "",
                            "low": ""
                        }
                    },
                    "shouldDetectModules": "",
                    "dontComputeNew": "",
                    "doNotResolveRelativePaths": ""
                },
                "locks": []
            },
            "deploy": {
                "publishedArtifacts": ["dbc-web/target/dbc-dc/**/*=>tld/company/dbc"],
                "params": []
            }
        },
        {
            "type": "DB",
            "tool": {
                "type": "maven",
                "version": "3.0.1"
            },
            "technology": {
                "type": "oracle",
                "version": ""
            },
            "scm": {
                "type": "svn",
                "base": "https://svn.company.tld/group/dbc"
            },
            "ci": {
                "locks": [
                    "DB_RELATED_LOCK",
                    "SOME_OTHER_LOCK"
                ],
                "shell": "ssh-agent sh -x ${SCRIPTSHOME}/bin/ci_db_dpl_parent.sh afe-db.dev.company.tld"
            },
            "deploy": {
                "publishedArtifacts": ["target/dbc_db-dc/**/*=>tld/company/dbc_db"],
                "params": []
            }
        }
    ]
}
